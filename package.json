{
  "name": "ijn",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.72.2",
    "eslint": "^8.55.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "^1.11.1",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "private": true,
  "resolutions": {
    "metro": "~0.76.7",
    "metro-resolver": "~0.76.7"
  },
  "scripts": {
    "native": "cd apps/expo-app && bun dev",
    "web": "cd apps/next-app && bun next",
    "prebuild:native": "cd apps/expo-app && bun prebuild",
    "clean": "git clean -xdf node_modules",
    "clean:all": "git clean -xdf node_modules bun.lockb && bun clean:expo && bun clean:next && bun clean:api && bun clean:app && bun clean:db",
    "clean:cache": "bun pm cache rm && rm -fr $TMPDIR/metro-cache && rm $TMPDIR/haste-map-*",
    "clean:expo": "cd apps/expo-app && bun clean",
    "clean:next": "cd apps/next-app && bun clean",
    "clean:api": "cd packages/api && bun clean",
    "clean:app": "cd packages/app && bun clean",
    "clean:db": "cd packages/db && bun clean",
    "android": "cd apps/expo-app && bun android",
    "ios": "cd apps/expo-app && bun ios"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.2",
    "react-native-url-polyfill": "^2.0.0"
  }
}
